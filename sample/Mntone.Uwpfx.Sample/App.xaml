<Application x:Class="Mntone.Uwpfx.Sample.App"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 StartupUri="MainWindow.xaml">
	<Application.Resources>
		<ResourceDictionary>
			<BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter" />

			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="pack://application:,,,/Uwpfx;component/Themes/Language/English.xaml" />
				<ResourceDictionary Source="pack://application:,,,/Uwpfx;component/Themes/Accent/Blue.xaml" />
				<ResourceDictionary Source="pack://application:,,,/Uwpfx;component/Themes/Base/Dark.xaml" />
				<ResourceDictionary Source="pack://application:,,,/Uwpfx;component/Themes/Generic.xaml" />
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources>
</Application>